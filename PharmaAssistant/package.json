{
  "name": "pharma-assistant",
  "version": "1.0.0",
  "description": "Regulatory Medical Writing Assistant for pharmaceutical professionals",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "cd frontend && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd frontend && npm install",
    "build": "cd frontend && npm run build",
    "prod": "NODE_ENV=production node backend/server.js",
    "lint": "eslint \"**/*.js\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "pharmaceutical",
    "regulatory",
    "medical",
    "writing",
    "assistant",
    "AI",
    "clinical trials",
    "FDA",
    "EMA"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "docxtemplater": "^3.40.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "html-to-pdfmake": "^2.5.1",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.6.0",
    "md-to-pdf": "^5.2.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pdfkit": "^0.14.0",
    "pizzip": "^3.1.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
